{% block javascripts %}
    <script src="{{ asset('js/searchMeme.js') }}" type="text/javascript" defer></script>
{% endblock %}
<header>
    <div class="search-bar">
        <form>
            <input placeholder="Search">
        </form>
    </div>
    {% if app.user %}
        <a class="login" href="{{ path('app_logout') }}">Logout</a>
    {% else %}
        <a class="login" href="{{ path('app_login') }}">Login</a>
    {% endif %}
</header>